<section onload="placeMarkers">
  <header class="search">
    <form action="/search" method="POST">
      <input type="text" name="location" id="location" placeholder="Where?">
      <input type="text" name="title" id="title" placeholder="What?">
      <button type="submit">Search</button>
    </form>
  </header>
  <div class="container" id="job-list">
    {{#each data.jobs}}
      {{> parJobDetails this }}
    {{/each}}
  </div>
  <div class="container" id="jobs-map"></div>
</section>